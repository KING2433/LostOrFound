
/* 主页 */  
.home {  
  position:fixed; 
  width: 100%;  
  height: 100%;  
  background-color: #ede2ad;  
  z-index: 1;  
  transition: All 0.4s ease;  
  -webkit-transform-origin: 0% 0%;
  /* -webkit-transition: All 0.4s ease; */
  overflow-y:scroll;
  overflow-x: hidden;
}  
.home .icon_user {  
  width: 120rpx;  
  height: 120rpx;  
  margin-left: 20rpx;  
  margin-top: 20rpx;  
  border-radius:118rpx;
   position: absolute;  
 box-shadow: 0rpx 0rpx 14rpx grey;  
}  
.siderbar-image{
   display: inline-block;
  width: 130rpx;
  height: 130rpx;
  border-radius: 128rpx;
  margin-top: 20rpx;
  vertical-align: middle;
  margin-left: 50rpx;
}
/* 遮罩层 */  
.bg {    
  display: none;    
  position: fixed;   
  top: 0%;    
  right: 80%;    
  width: 100%;    
  height: 100%;
  -webkit-transform-origin: 100% 100%;    
  background-color: black;    
  z-index: 1001;    
  -moz-opacity: 0.7;    
  opacity: 0.70;    
  transition:width 2s;  
  filter: alpha(opacity=70);    
}   
 /* 侧滑栏 */  
.page_tab {  
  height: 100%;
  margin-left: 20%; 
  width: 80%;  
  position: fixed;
  overflow-y: scroll;
  overflow-x: hidden;  
  background-color:#ede2ad;
  -webkit-transform-origin: 100% 100%;  
  z-index: 0; 
}  
  
.page_tab_content .wc {  
  color: #000;  
  position: relative;  
  font-size: 32rpx;  
  padding: 20rpx 0 30rpx 20rpx;  
}  
  
.page_tab_content button {  
  width: 280rpx;  
  background: #fb9817;  
  color: #fff;  
  position: relative;  
  right: 120rpx;  
  top: 150rpx;  
  height: 60rpx;  
  line-height: 60rpx;  
}  
  
.page_tab_content image {  
  display: inline-block;  
  height: 40rpx;  
  width: 40rpx;  
  margin-right: 60rpx;  
}  
  
.user_head {  
  width: 120rpx;  
  height: 120rpx;  
  border-radius: 50%;  
  text-align: center;  
  margin: 80rpx 80rpx 80rpx 205rpx;  
}  

