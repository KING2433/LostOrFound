.welcome-container{
	display: flex;
	flex-direction: column;
	align-items: center;
}

.welcome-icon{
	width: 100%;
	height: 640rpx;
}

.welcome-icon-image{
	width: 100%;
	height: 640rpx;
}

.welcome-content{
	margin-top: 10rpx;
	color: #366a92;
	font-size: 50rpx;
}

.welcome-button{
	margin-top: 60rpx;
	width: 260rpx;
	height: 100rpx;
	border: solid; 
	border-width:6rpx;
	border-radius: 15rpx;  
	font-size: 45rpx;
	line-height: 90rpx;
	border-color: #366a92; 
	background-color: #faf2ce; 
	color: #366a92;
}

.welcome-button:hover{
	background-color: #366a92;
	color:white;
}

.welcome-team{
	margin-top: 260rpx;
	font-size: 40rpx;
	color: #e4b088;
}

page{
  height: 100%;
